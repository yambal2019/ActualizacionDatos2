@model ActualizacionDatosCampaña.Models.DatoModel
@{
    ViewBag.Title = "Datos";
    Layout = "~/Views/Shared/_Master.cshtml";
}



<div class="col-lg-12 register-page">
    <div class="theme-card">
        @using (Html.BeginForm("Consultora", "Datos", FormMethod.Post, new { @class = "theme-form" }))
        {

            <div class="dashboard">
                <div class="box-head">
                    <h2>  Hola, @Model.vchNombre </h2>
                </div>

            </div>

            <div class="texto">
                <h4> Comencemos.. </h4>
            </div>
            <input type="text" name="vchTelefono" id="vchTelefono" class="form-control" placeholder="Nro de Telefono" />

            <p class="submit">
                <input type="submit" name="wp-submit" id="btnsubmit" class="btn btn-primary btn-border" value="Enviar..." />
            </p>

        }
    </div>
</div>



